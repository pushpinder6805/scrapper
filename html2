  $("#iframe").height($("#parnumberdiv p").length * 28);
  
  $("#parnumberdiv").css("top",$("#searchkey_rfq").offset().top + $("#searchkey_rfq").outerHeight());
  $("#parnumberdiv").css("left",$("#searchkey_rfq").offset().left);
  $("#parnumberdiv").css("width",$("#searchkey_rfq").outerWidth() - 14);
  }
  else
  {
  closenumber();
  }
  });
  }
  else
  {
  closenumber();
