<script>
function compare_add(id, obj, name, url)
{
$.get("ajax_compare.php?ac=compare_add", { id:id, name:name, url:url, rst:randomstr()}, function(data){
if (data == 'error')
{
alert("Sorry, you've added 15 models, please delete at least one model before adding.");
}
else
{
if (obj != 1)
{
var str = obj.innerHTML;
if (str.indexOf("plus14.gif") > 0)
{
obj.innerHTML = '<img src="cms-templates/en/images/compare/minus14.gif" width="14" height="14" alt="Delete" />';	
}
else
{
obj.innerHTML = '<img src="cms-templates/en/images/compare/plus14.gif" width="14" height="14" alt="Add" />';	
}
}

compare_get();
$("#compare_box").show();
$("#compare_mini").hide();
}
});
}

function compare_del(id, name)
{
$("#panel_list_comp_"+id).html('<img src="cms-templates/en/images/compare/plus14.gif" width="14" height="14" alt="Add" />');
$.get("ajax_compare.php?ac=compare_add", { id:id, name:name, url:'', rst:randomstr()}, function(data){
//compare_get();
$("#compare_li_"+id).remove();
});
}
function compare_get()
{
var str = '';
$.getJSON("ajax_compare.php?ac=compare_get", {rst:randomstr()}, function(data){
if (data.comp_close == 1)
{
$("#compare_box").hide();
$("#compare_mini").hide();
}
else
{
if (data.nums > 0)
{
$.each(data.items, function(key, val) {
str += '<li id="compare_li_'+val[0]+'">';
str += '<input class="selectbox" type="hidden" name="panelids[]" value="'+val[0]+'" />';
str += '<img onclick="compare_del('+val[0]+', \''+val[1]+'\')" src="cms-templates/en/images/compare/del.gif" width="15" height="15" title="Delete" />';
//str += "<a href=\""+val[2]+"\">"+val[1]+"</a>";
str += "<a href=\"modeldetail.php?id="+val[0]+"\">"+val[1]+"</a>";
str += '</li>';

$("#panel_list_comp_"+val[0]).html('<img src="cms-templates/en/images/compare/minus14.gif" width="14" height="14" alt="Delete" />');
});
$("#compare_submit").show();
$("#compare_empty").hide();

if (data.comp_hide == 1)
{
compare_hide();	
}
else
{
compare_show();
}
}
else
{
$("#compare_submit").hide();
$("#compare_empty").show();
$("#compare_mini").hide();
}
$("#compare_mod_list").html(str);
}
$("#compare_num").html(data.nums);
});
}
compare_get();

function compare_clear()
{
$.getJSON("ajax_compare.php?ac=compare_get", {rst:randomstr()}, function(data){
if (data.nums > 0)
{
$.get("ajax_compare.php?ac=compare_clear", { rst:randomstr()}, function(data){});
$.each(data.items, function(key, val) {
//$.get("ajax_compare.php?ac=compare_add", { id:val[0], name:val[1], url:'', rst:randomstr()}, function(data){});
$("#panel_list_comp_"+val[0]).html('<img src="cms-templates/en/images/compare/plus14.gif" width="14" height="14" alt="Add" />');
});
}
$("#compare_mod_list").html('');
$("#compare_submit").hide();
$("#compare_empty").show();
$("#compare_num").html('0');
});
}
function compare_hide()
{
$.get("ajax_compare.php?ac=compare_hide", {rst:randomstr()}, function(data){
$("#compare_box").hide();
$("#compare_mini").show();
initFloatTips();
});
}
function compare_show()
{
$.get("ajax_compare.php?ac=compare_show", {rst:randomstr()}, function(data){
$("#compare_box").show();
$("#compare_mini").hide();
initFloatTips();
});
}

function compare_cose()
{
$.get("ajax_compare.php?ac=compare_cose", {rst:randomstr()}, function(data){
$("#compare_box").hide();
$("#compare_mini").hide();
$(".panel_list_comp").html('<img src="cms-templates/en/images/compare/plus14.gif" width="14" height="14" alt="Add" />');
});
}

var tips; 
var theTop = 95; 
var old = theTop;
function initFloatTips()
{
if ($("#compare_box").is(":hidden") == true)
{
tips = document.getElementById('compare_mini');
}
else
{
tips = document.getElementById('compare_box');
}
moveTips();
}
function moveTips()
{
var tt=50;
if (window.innerHeight)
{
pos = window.pageYOffset
}
else if (document.documentElement && document.documentElement.scrollTop)
{
pos = document.documentElement.scrollTop
}
else if (document.body)
{
pos = document.body.scrollTop;
}
pos=pos-tips.offsetTop+theTop;
pos=tips.offsetTop+pos/10;
if (pos < theTop) pos = theTop;
if (pos != old)
{
tips.style.top = pos+"px";
tt=10;
}
old = pos;
setTimeout(moveTips,tt);
}
initFloatTips();

//询价蓝数量
