<script>
$("#para_nav_line").height($("#para_nav_li").children("li").length * 30 - 30);

//显示更多现货和供应商
function show_all_ea(divid, moreid)
{
$("."+divid).find(".item").show();
$("."+moreid).hide();
}


//板块的高度
var ModelTopArr = new Array();
$(".model_part").each(function(index, element){
ModelTopArr.push(parseInt($(this).offset().top));

});
console.log(ModelTopArr)

// 监听页面滚动
$(window).scroll(function() {

var now_i = 0;//当前锚点

for(var i=0; i< ModelTopArr.length; i++)
{
if($(this).scrollTop() > (ModelTopArr[i] - 50))
{ 
now_i = i;
}
}

for(var i=0; i< ModelTopArr.length; i++)
{
if (now_i == i)
{
$('#para_nav_li li a').eq(i).addClass('cur');
}
else
{
$('#para_nav_li li a').eq(i).removeClass('cur');
}
}

});
</script>
