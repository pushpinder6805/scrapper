/* Pop member sign in window 
function ajax_login_box()
{
$(".login_box_bg").show();
$(".login_box_bg").css("width",$(document.body).width());
$(".login_box_bg").css("height",$(document.body).height());

$("#login_box").show();

var top = (($(window).height() - 260)/2) + $(document).scrollTop();
var left = (($(window).width() - 400)/2) + $(document).scrollLeft();
$("#login_box .login_box").css("top",top);
$("#login_box .login_box").css("left",left);
$("#login_box .iframe_login").css("top",top);
$("#login_box .iframe_login").css("left",left);

var company_url = '';
if (company_url)
{
refreshcode(company_url);
}
else
{
refreshcode('https://panelook.com');
}
}
*/

function login_box_check()
{
var username = $("#lbusername").val();
var password = $("#lbpassword").val();
var vcode 	 = $("#lbvcode").val();

if (username == '')
{
alert("Please enter username!");
$("#lbusername").focus();
return false;
}
if (password == '')
{
alert("Please enter password!");
$("#lbpassword").focus();
return false;
}
<!---->


//显示loading图标
$(".lbdiv .loading").show();

$.post("login_box.php?ac=checklogin&lang=en", { username:username, password:password, vcode:vcode, rst:randomstr()}, function(data)
{
if (data == '1')
{
alert("Incorrect username or password!");
//$("#lbusername").val('');
$("#lbpassword").val('');
$(".lbdiv .loading").hide();
$("#lbpassword").focus();
return false;
}
else if (data == '2')
{
alert("Incorrect verification code!");
$("#lbvcode").val('');
$(".lbdiv .loading").hide();
$("#lbvcode").focus();
return false;
}
else if (data == '3')
{
alert("Your account disabled!");
$("#lbvcode").val('');
$(".lbdiv .loading").hide();
$("#lbvcode").focus();
return false;
}
else
{
doOpenPostIfrm("http://www.panelook.cn/login_box.php?ac=checklogin_cooike&lang=cn", [{ name: "username", value: username}, { name: "password", value: password}]);
setTimeout(function (){
location.reload();
$(".login_box_bg").hide();$("#login_box").hide();
}, 600);
}
});
}



function cmsbox_close()
{
$(".cmsbox_bg").hide();
$("#cmsbox").hide();
}

function cmsok_box(title, url, width, height)
{
$(".cmsbox_bg").show();
$(".cmsbox_bg").css("width",$(document.body).width());
$(".cmsbox_bg").css("height",$(document.body).height());

if (width > 0)
{

}
else
{
width = 500;	
}

if (height > 0)
{

}
else
{
height = 300;	
}

$("#cmsbox").show();
var top = (($(window).height() - height)/2) + $(document).scrollTop();
var left = (($(window).width() - width)/2) + $(document).scrollLeft();
$("#cmsbox .cmsbox").css("top",top);
$("#cmsbox .cmsbox").css("left",left);

$("#cmsbox .cmsbox").css("width",width);
$("#cmsbox .cmsbox").css("height",height);

$("#cmsbox iframe").css("width",width);
$("#cmsbox iframe").css("height",height - 30);


$("#cmsbox #cmsbox_title").html(title);
$("#cmsbox iframe").attr("src",url);
}
function down_pdf(id)
{	
if (uid > 0)
{
cmsok_box('System Prompts', 'down.php?ac=pdf&id='+id+'&s='+randomstr());
}
else
{
ajax_login_box();
}
}
function down_iis(id)
{	
if (uid > 0)
{
cmsok_box('System Prompts', 'down.php?ac=iis&id='+id+'&s='+randomstr());
}
else
{
ajax_login_box();
}
}

function down_connector_pdf(id)
{	
if (uid > 0)
{
location.href = 'down_connector_pdf.php?id='+id+'&s='+randomstr();
//cmsok_box('提示消息', 'down_connector_pdf.php?id='+id+'&s='+randomstr());
}
else
{
ajax_login_box();
}
}

/* Panel model list customized setup */
function check_attr(obj)
{
var s = 0;
var str = '';
$(".posinline").each(function(index, element) {
if ($(this).is(':checked') == true)
{
s += 1;
}
    });	

if (s > 9)
{
obj.checked = false;
alert("You can at most choose 9 items.");
return ;	
}
}
function select_attr()
{
var s = 0;
var str = '';
$(".posinline").each(function(index, element) {
if ($(this).is(':checked') == true)
{
s += 1;
str += $(this).val() + '-';
}
    });	

if (s > 9)
{
alert("You can at most choose 9 items.");
return ;	
}

$.get("panel_list_attr.php?ac=set_cookie", { str:str, panel_show_key:'', rst:randomstr()}, function(data){
 location.reload() 
});
}
function clear_attr()
{
$.get("panel_list_attr.php?ac=clear_cookie", { panel_show_key:'', rst:randomstr()}, function(data){
 location.reload() 
});
}

function toggle_usersetup()
{
$(".usersetup").toggle();	
}

$(document).keyup(function(event){
if(event.keyCode ==13){
if ($("#login_box").is(':hidden') == false)
{
login_box_check()
}
}
});
</script>
