        <script type="text/javascript">
function checkpdffrom()
{
var searchkey_panel = document.getElementById("searchkey_panel").value;
if (searchkey_panel.length < 2)
{
alert("Please enter product name or panel model");
document.getElementById("searchkey_panel").focus();
return false;
}
}
function checkinvform()
{
var searchkey_inv = document.getElementById("searchkey_inv").value;
if (searchkey_inv.length < 3)
{
alert("Please enter panel model");
document.getElementById("searchkey_inv").focus();
return false;
}
}
function checkrfqform()
{
var searchkey_rfq = document.getElementById("searchkey_rfq").value;
if (searchkey_rfq.length < 3)
{
alert("Please enter panel model");
document.getElementById("searchkey_rfq").focus();
return false;
}
}
function checksupform()
{
var searchkey_sup = document.getElementById("searchkey_sup").value;
if (searchkey_sup.length < 2)
{
alert("Please enter company name");
document.getElementById("searchkey_sup").focus();
return false;
}
}
function selectst(type)
{
$("#nstdl").children("dd").hide();
if (type == 1)
{
$("#nstdl dt").html('Products');
}
if (type == 2)
{
$("#nstdl dt").html('Stocks');
}
if (type == 3)
{
$("#nstdl dt").html('RFQs');
}
if (type == 4)
{
$("#nstdl dt").html('Suppliers');
}
for (var i = 1 ; i <= 4 ; i ++)
{
if (type == i)
{
$("#nstdiv"+i).show();
}	
else
{
$("#nstdiv"+i).hide();
}
}
$("#search_type").val(type);	
}

//关联Search
function selectnumber_pdf(str)
{
  if (isChinese(str))
  {
  return false;
  }
  if (str.length > 5)
  {
  $.get("ajax.php?ac=searchnumber&type=pdf&lang=en", { str:str,  rands:randomstr ()}, function(data)
  {
  if (data)
  {
  $("#parnumberdiv").html(data + '<iframe id="iframe" width="100%" frameborder="0" marginwidth="0" marginheight="0" scrolling="no"></iframe>');
  $("#parnumberdiv").show();
  $("#iframe").height($("#parnumberdiv p").length * 28);
  
  $("#parnumberdiv").css("top",$("#searchkey_panel").offset().top + $("#searchkey_panel").outerHeight());
  $("#parnumberdiv").css("left",$("#searchkey_panel").offset().left);
  $("#parnumberdiv").css("width",$("#searchkey_panel").outerWidth() - 14);
  }
  else
  {
  closenumber();
  }
  });
  }
  else
  {
  closenumber();
  }
}

//关闭面板型号图层
function closenumber()
{
$("#parnumberdiv").hide();
}

//选择型号
function thisnumber(str)
{
var type = $("#search_type").val();

if (type == 1)
{
window.location.href='https://panelook.com/go.php?keyword='+str;
}
if (type == 2)
{
window.location.href='https://panelook.com/inventories.php?keyword='+str;
}
if (type == 3)
{
window.location.href='https://panelook.com/rfqs.php?keyword='+str;
}



window.event.returnValue = false;
}


//关联Search
function selectnumber_inv(str)
{
  if (isChinese(str))
  {
  return false;
  }
  if (str.length > 5)
  {
  $.get("ajax.php?ac=searchnumber&type=inv&lang=en", { str:str,  rands:randomstr ()}, function(data)
  {
  if (data)
  {
  $("#parnumberdiv").html(data + '<iframe id="iframe" width="100%" frameborder="0" marginwidth="0" marginheight="0" scrolling="no"></iframe>');
  $("#parnumberdiv").show();
  $("#iframe").height($("#parnumberdiv p").length * 28);
  
  $("#parnumberdiv").css("top",$("#searchkey_inv").offset().top + $("#searchkey_inv").outerHeight());
  $("#parnumberdiv").css("left",$("#searchkey_inv").offset().left);
  $("#parnumberdiv").css("width",$("#searchkey_inv").outerWidth() - 14);
  }
  else
  {
  closenumber();
  }
  });
  }
  else
  {
  closenumber();
  }
}
//关联Search
