<script>
//标签
function set_tags(company_id, app_type, app_id)
{
var url = "models.php?op=company-tags&ac=info&company_id="+company_id+"&app_type="+app_type+"&app_id="+app_id;

if (uid == '')
{
ajax_login_box(url, 550, 330);
return false;
}


layui.use('layer', function(){
var layer = layui.layer;
layer.open({
  type: 2,
  title: false,
  skin: 'layui-layer-rim', //加上边框
  area: ['550px', '330px'], //宽高
  content: url
});	
});
}

//联系方式
function com_contact(company_id, app_type, app_id)
{
var url = "models.php?op=company-contact&ac=info&lang=en&company_id="+company_id+"&app_type="+app_type+"&app_id="+app_id;
layui.use('layer', function(){
var layer = layui.layer;
layer.open({
  type: 2,
  title: false,
  closeBtn:false,
  skin: 'layui-layer-rim', //加上边框
  area: ['1000px', '540px'], //宽高
  content: url
});	
});
}

//加入询价篮
function model_add_inquiry(type, val)
{
if (uid == 0)
{
ajax_login_box();
return false;
}

var post_arr = new Array();
post_arr.push(type+':'+val);

$.post("ajax.php?ac=add_inquiry_en", { post_arr:post_arr,  rands:randomstr () }, function(data){
if (data > 0)
{
$(".top_msg_num").html(data);
//show_tips("成功加入询价篮");
}

});	
}
</script>
