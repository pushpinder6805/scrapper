<script type="text/javascript">
$(document).ready(function () {
para_nav_position();

//左侧导航点击滑动效果
$("#para_nav_li a").each(function(index, element) {
        $(this).click(function(){
$('body,html').animate({ scrollTop: $('.model_part').eq(index).offset().top - 8 }, 360);
});
    });
});
$(window).resize(function(){
para_nav_position()	
});



function para_nav_position()
{
var window_width = $(window).width();
if (window_width < 1280)
{
$(".para_nav").hide();
}
else
{
var left = (window_width - 1200) / 2 - 95;
$(".para_nav").css("left", left + 'px');
$(".para_nav").show(300);
}
}

function cmsbox_close_down()
{
setTimeout(function(){

$(".cmsbox_bg").hide();
$("#cmsbox").hide();

},500);
}
</script>


