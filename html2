//通用函数
function msg_top(content, icon)
{
layui.use('layer', function(){
var layer = layui.layer;

layer.msg(content,{offset: '30px', time :1500});
});   
}
function urlencode(str)
{  
    str = (str + '').toString();   
    return encodeURIComponent(str).replace(/!/g, '%21').replace(/'/g, '%27').replace(/\(/g, '%28').  
    replace(/\)/g, '%29').replace(/\*/g, '%2A').replace(/%20/g, '+');  
}

//会员登录
function ajax_login_box(block_url, width, height)
{
block_url = (typeof(block_url) !== "undefined") ? urlencode(block_url) : '';

var url = "models.php?op=login&ac=login&lang=en&block_url="+block_url+"&width="+width+"&height="+height+"";
layui.use('layer', function(){
var layer = layui.layer;
layer.open({
  type: 2,
  title: false,
  shade: 0.7,
  area: ['450px', '380px'], //宽高
  skin: 'layui-layer-rim', //加上边框
  content: url,
  cancel: function(){
//page_footer_load(); //从新加载页面
  } 
});	
});
}


//页面底部加载
function page_footer_load()
{
var param = {
page_type:'modeldetail',
page_id:'45369'
}


$.getJSON("models.php?op=footer-load&ac=load&lang=en", param, function(result)
{
console.log(result)
var member = result.data.member;
var msg_nums = result.data.msg_nums;
var tags = result.data.tags;

//会员状态
if (member.uid > 0)
{
uid = member.uid;
var _top_member_info = '<li class="left"><a href="member.php">Hi! '+member.username+'</a>&nbsp;<a href="member.php?action=logout">[Sign Out]</a> &nbsp; <a href="member.php?op=inquiry" class="inmsgs"><span class="orange">'+msg_nums.msgs_inbox+'</span> new</a></li>';
}
else
{
var _top_member_info = '<li class="left user"><a href="https://panelook.com/register.php">Join Free</a> | <a href="javascript:;" title="Sign In" onclick="ajax_login_box()">Sign In</a></li>';
}


$("#top_member_info").html(_top_member_info);
if (msg_nums)
{
$(".top_msg_num").html(msg_nums.xunjialan);
}

//标注
if (param.page_type == 'modeldetail' && tags)
{
for (var invid in tags.inv)
{
$("#tag_inv_"+invid).html(tags.inv[invid])
}
for (var supid in tags.sup)
{
$("#tag_sup_"+supid).html(tags.sup[supid])
}
}



//VIP买家查看价格
if (param.page_type == 'modeldetail' && result.data.view_content == 1)
{
var invitems = result.data.invitems;
for (var key in invitems)
{
var price_html = (invitems[key].inv_currency == 'USD') ? '$' : '￥';
price_html += invitems[key].inv_quotation + '/pcs';

$("#view_price_"+invitems[key].invid).html(price_html)
}
}
});
}
</script>
<script type="text/javascript">

$(document).ready(function()
{
$(".tablelist tr").each(function()
{
$(this).hover(function(){$(this).toggleClass("set");},function(){$(this).toggleClass("set");});
});
$(".serieslist tr").each(function()
{
$(this).hover(function(){$(this).toggleClass("set");},function(){$(this).toggleClass("set");});
});
$(".selectbox").each(function()
{
$(this).click(function(){
if ($(this).attr("checked") == true){$(this).parent("td").parent("tr").children("td").addClass("set");}
else{$(this).parent("td").parent("tr").children("td").removeClass("set");	}
});
});

$("#nstdl dt").click(function(){
if ($("#nstdl").children("dd").is(':hidden') == true)
{
//$("#nstdl").children("dd").slideDown(200);
$("#nstdl").children("dd").show();
}
else
{
//$("#nstdl").children("dd").slideUp(200);
$("#nstdl").children("dd").hide();
}
});
});



/* 面板型号检索页面下拉参数展开/收起切换 */
$(document).ready(function(){
$(".eamore").click(function(){
if ($(this).hasClass("moreup"))
{
$(this).parent().height(min_height);
$(this).html("More");
}
else
{
$(this).parent().height("auto");
$(this).html("Less");
}
$(this).toggleClass("moreup");
});

$(".ea_dls dl").hover(function(){
if ($(this).children("dd").is(':hidden') == true)
{
$(this).children("dd").slideDown(200);
}
else
{
$(this).children("dd").slideUp(200);
}
$(this).toggleClass("cur");
},function(){
$(this).children("dd").slideUp(200);
$(this).toggleClass("cur");
});
});



